[assume a 1]
[assume b 1.5]
[assume c (/ 1 2)]
[assume d true]
[assume e "hello world"]
[assume f (lambda (x) (pow x 2))]
[assume g (mem (lambda (x) (f x)))]
[assume h even?]

[predict (f 4)]
[predict (g 4)]

[predict (apply f (list 4))]
[predict (apply g (list 4))]

[predict (list 'proc? a ': (proc? a))]
[predict (list 'proc? b ': (proc? b))]
[predict (list 'proc? c ': (proc? c))]
[predict (list 'proc? d ': (proc? d))]
[predict (list 'proc? e ': (proc? e))]
[predict (list 'proc? f ': (proc? f))]
[predict (list 'proc? g ': (proc? g))]
[predict (list 'proc? h ': (proc? h))]
